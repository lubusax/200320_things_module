<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Menus  -->
    <menuitem
        id="things_menu_root" 
        name="Things Network"
    />
    
    <act_window
        id="things_gate_action" 
        name="Gates"
        res_model="things.gate"
        view_mode = "tree,form"
    />
    
    <menuitem
        id="things_menu_gate"
        name="Gates"
        action="things_gate_action" 
        parent="things_menu_root"
    />

    <act_window
        id="things_thing_action" 
        name="Things"
        res_model="base.thing"
        view_mode = "tree,form"
    />

    <menuitem
        id="things_menu_thing"
        name="Things"
        action="things_thing_action"
        parent="things_menu_root"
    />

</odoo>